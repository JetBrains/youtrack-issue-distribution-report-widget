{
  "name": "youtrack-agile-chart-widget",
  "version": "1.0.0",
  "private": true,
  "config": {
    "components": "./src",
    "dist": "./dist"
  },
  "scripts": {
    "postinstall": "npm rebuild node-sass",
    "create-component": "yo @jetbrains/ring-ui:react --path $npm_package_config_components",
    "build": "webpack -p",
    "test": "karma start",
    "ci-test": "xvfb-maybe karma start --teamcity",
    "start": "webpack-dev-server -d --history-api-fallback --inline --hot --colors --port 9012",
    "i18n:extract": "angular-gettext-cli --files \"{src/**/*.js,../../components/src/**/*.js,node_modules/hub-dashboard-addons/components/**/*.js,node_modules/@jetbrains/hub-widget-ui/src/**/*.js}\" --dest src/app/translations/youtrack-agile-chart-widget.pot --marker-name=i18n",
    "clean": "rm -rf node_modules"
  },
  "babel": {
    "presets": "@jetbrains/jetbrains"
  },
  "dependencies": {
    "@jetbrains/babel-preset-jetbrains": "1.0.1",
    "@jetbrains/hub-widget-ui": "2.0.1",
    "@jetbrains/icons": "3.6.0",
    "@jetbrains/logos": "^1.1.6",
    "@jetbrains/ring-ui": "3.0.19",
    "angular-gettext-cli": "1.2.0",
    "angular-gettext-loader": "1.0.1",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-polyfill": "6.26.0",
    "classnames": "2.2.6",
    "css-loader": "3.0.0",
    "d3": "3.5.16",
    "fecha": "2.3.3",
    "html-loader": "0.4.4",
    "hub-dashboard-addons": "0.2.0",
    "json-loader": "0.5.7",
    "mout": "0.11.0",
    "node-sass": "7.0.0",
    "nvd3": "github:JetBrains/nvd3",
    "postcss-cssnext": "3.1.0",
    "postcss-loader": "2.0.9",
    "postcss-modules-values-replace": "1.2.1",
    "prop-types": "15.6.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "redbox-react": "1.3.6",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "webpack": "4.28.1",
    "webpack-dev-server": "3.1.14"
  },
  "devDependencies": {
    "@jetbrains/stylelint-config": "^3.0.2",
    "chai": "4.0.2",
    "chai-as-promised": "^7.1.1",
    "chai-dom": "1.5.0",
    "chai-enzyme": "1.0.0-beta.1",
    "chai-spies": "1.0.0",
    "copy-webpack-plugin": "4.6.0",
    "electron": "^13.6.8",
    "enzyme": "3.6.0",
    "enzyme-adapter-react-16": "1.5.0",
    "html-webpack-plugin": "3.2.0",
    "karma": "^6.3.12",
    "karma-chai-plugins": "0.9.0",
    "karma-electron-launcher": "0.2.0",
    "karma-mocha": "^2.0.1",
    "karma-sourcemap-loader": "0.3.7",
    "karma-teamcity-reporter": "1.1.0",
    "karma-webpack": "3.0.5",
    "lerna": "^4.0.0",
    "mocha": "^9.2.0",
    "react-test-renderer": "^17.0.2",
    "sinon": "2.3.5",
    "sinon-chai": "2.11.0",
    "stylelint": "^14.3.0",
    "webpack-cli": "3.2.1",
    "xvfb-maybe": "0.2.1"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
