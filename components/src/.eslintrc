{
  "parser": "babel-eslint",
  "root": true,
  "extends": [
    "@jetbrains",
    "@jetbrains/eslint-config/es6",
    "@jetbrains/eslint-config/browser",
    "@jetbrains/eslint-config/react",
    "@jetbrains/eslint-config/test"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "import/no-unresolved": "off",
    "import/extensions": "off"
  }
}
